<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/home_app_mode_youtube"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/btn_home_youtube_1"
        app:layout_constraintBottom_toTopOf="@+id/workout_mode_quick_start"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/home_app_mode_chrome"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.502"
        app:layout_constraintVertical_weight="1" />

    <ImageView
        android:id="@+id/home_app_mode_chrome"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/btn_home_chrome_1"
        app:layout_constraintBottom_toTopOf="@+id/workout_mode_quick_start"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toRightOf="@+id/home_app_mode_youtube"
        app:layout_constraintRight_toLeftOf="@+id/home_app_mode_facebook"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.502"
        app:layout_constraintVertical_weight="1" />

    <ImageView
        android:id="@+id/home_app_mode_facebook"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/btn_home_facebook_1"
        app:layout_constraintBottom_toTopOf="@+id/workout_mode_quick_start"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toRightOf="@+id/home_app_mode_chrome"
        app:layout_constraintRight_toLeftOf="@+id/home_app_mode_flikr"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_weight="1" />

    <ImageView
        android:id="@+id/home_app_mode_flikr"
        android:layout_width="0dp"
        android:layout_height="00dp"
        android:src="@drawable/btn_home_flikr_1"
        app:layout_constraintBottom_toTopOf="@+id/workout_mode_quick_start"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toRightOf="@+id/home_app_mode_facebook"
        app:layout_constraintRight_toLeftOf="@+id/home_app_mode_instagram"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.502"
        app:layout_constraintVertical_weight="1" />

    <ImageView
        android:id="@+id/home_app_mode_instagram"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/btn_home_instagram_1"
        app:layout_constraintBottom_toTopOf="@+id/workout_mode_quick_start"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toRightOf="@+id/home_app_mode_flikr"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.502"
        app:layout_constraintVertical_weight="1" />

    <ImageView
        android:id="@+id/workout_mode_quick_start"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:src="@drawable/btn_home_quickstart_1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home_app_mode_facebook"
        app:layout_constraintVertical_weight="1" />

</android.support.constraint.ConstraintLayout>
