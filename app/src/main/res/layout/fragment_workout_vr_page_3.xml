<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="5dp"
    android:paddingRight="5dp">

    <ImageView
        android:id="@+id/workout_mode_vr_img_9"
        android:layout_width="0dp"
        android:layout_height="120dp"
        android:src="@drawable/btn_home_vr_9"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/workout_mode_vr_img_10"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.35" />


    <ImageView
        android:id="@+id/workout_mode_vr_img_10"
        android:layout_width="0dp"
        android:layout_height="120dp"
        android:layout_marginLeft="5dp"
        android:src="@drawable/btn_home_vr_10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toRightOf="@+id/workout_mode_vr_img_9"
        app:layout_constraintRight_toLeftOf="@+id/view2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.35" />

    <View
        android:id="@+id/view2"
        android:layout_width="0dp"
        android:layout_height="120dp"
        android:layout_marginLeft="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_weight="2"
        app:layout_constraintLeft_toRightOf="@+id/workout_mode_vr_img_10"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.35" />


    <TextView
        android:id="@+id/workout_mode_vr_name_9_1"
        style="@style/vr_item_text_1"
        android:text="@string/vr_i_1"
        android:textColor="@color/workout_mode_white"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/workout_mode_vr_name_10_1"
        app:layout_constraintTop_toBottomOf="@+id/workout_mode_vr_img_9" />


    <TextView
        android:id="@+id/workout_mode_vr_name_10_1"
        style="@style/vr_item_text_2"
        android:text="@string/vr_j_1"
        android:textColor="@color/workout_mode_white"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toRightOf="@+id/workout_mode_vr_name_9_1"
        app:layout_constraintRight_toLeftOf="@+id/view3"
        app:layout_constraintTop_toBottomOf="@+id/workout_mode_vr_img_10" />

    <View
        android:id="@+id/view3"
        android:layout_width="0dp"
        android:layout_height="10dp"
        android:layout_marginLeft="5dp"
        app:layout_constraintHorizontal_weight="2"
        app:layout_constraintLeft_toRightOf="@+id/workout_mode_vr_name_10_1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2" />

    <TextView
        android:id="@+id/workout_mode_vr_name_9_2"
        style="@style/vr_item_text_1"
        android:text="@string/vr_i_2"
        android:textColor="@color/workout_mode_white"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/workout_mode_vr_name_10_2"
        app:layout_constraintTop_toBottomOf="@+id/workout_mode_vr_name_9_1" />


    <TextView
        android:id="@+id/workout_mode_vr_name_10_2"
        style="@style/vr_item_text_2"
        android:text="@string/vr_j_2"
        android:textColor="@color/workout_mode_white"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toRightOf="@+id/workout_mode_vr_name_9_2"
        app:layout_constraintRight_toLeftOf="@+id/view4"
        app:layout_constraintTop_toBottomOf="@+id/workout_mode_vr_name_10_1" />


    <View
        android:id="@+id/view4"
        android:layout_width="0dp"
        android:layout_height="10dp"
        android:layout_marginLeft="5dp"
        app:layout_constraintHorizontal_weight="2"
        app:layout_constraintLeft_toRightOf="@+id/workout_mode_vr_name_10_2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view3" />
</android.support.constraint.ConstraintLayout>
